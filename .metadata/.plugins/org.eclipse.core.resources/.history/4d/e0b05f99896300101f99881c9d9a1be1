package com.demo.exception;

public class demo1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//exception handling
		System.out.println("First line");
		
		try {
			System.out.println(10/0);
		}
		catch(Exception e) {
			System.out.println(10/6);
		}
		System.out.println("Second line");
	}

}
